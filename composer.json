{
    "name": "bgaze/laravel-php-cs-fixer",
    "description": "A PHP-CS-Fixer bridge for use via Artisan CLI on Laravel 5.5+",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.5.0",
        "friendsofphp/php-cs-fixer": "2.3.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bgaze",
            "email": "benjamin@bgaze.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Bgaze\\LaravelPhpCsFixer\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Bgaze\\LaravelPhpCsFixer\\": "src"
        }
    }
}
